{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "enum": [
                "ack",
                "fail",
                "noack",
                "success"
            ]
        },
        "sequence_id": {
            "type": "number",
            "description": "The command sequence_id as generated by the CSC."
        }
    },
    "required": [
        "id",
        "sequence_id"
    ],
    "additionalProperties": false
}
